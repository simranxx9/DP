       int solve(int arr[], int i, int j)
    {
        if(i >= j) return 0;
        int ans = INT_MAX;
        for(int k = i; k <j; k++)
        {
            int temp_ans = solve(arr, i, k) + solve(arr, k+1, j) +
                arr[i-1]*arr[k]*arr[j];
                
            ans = min(ans, temp_ans);
        }
        return ans;
    }

    int matrixMultiplication(int N, int arr[])
    {
        // code here
        return solve(arr, 1, N-1);
    }
