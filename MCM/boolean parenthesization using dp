int dp[200][200][2];
    int solve(string s , int i, int j, bool isTrue)
    {
        //step-2 base condition
        if(i > j) // string is empty
            return 0;
        //check a variable    
        if(i == j)
        {
            if(isTrue)
                return s[i] == 'T';
            else
                return s[i] == 'F';
        }
        int ans = 0;
        int lf,lt,rt,rf;
        if(dp[i][j][isTrue]!= -1) return dp[i][j][isTrue];
        // step-3 for loop
        for(int k = i+1; k<j ; k+= 2)
        {
            if(dp[i][k-1][1] !=-1)
                lt = dp[i][k-1][1];
            else 
                lt = solve(s, i, k-1,1);
            if(dp[i][k-1][0] !=-1)
                lf = dp[i][k-1][0];
            else 
                lf = solve(s, i, k-1, 0);
            if(dp[k+1][j][1] !=-1)
                rt = dp[k+1][j][1];
            else 
                rt = solve(s, k+1, j, 1);
            if(dp[k+1][j][0] !=-1)
                rf = dp[k+1][j][0];
            else 
                rf = solve(s, k+1, j, 0);
            
            
            // step-4 write ans
            if(s[k] == '|')
            {
                if(isTrue)
                {
                    ans += lt*rt + rt*lf + rf*lt;
                }else{
                    ans += lf*rf;
                }
            }
            else if(s[k] == '&')
            {
                if(isTrue ==false)
                {
                    ans += lf*rf + rt*lf + rf*lt;
                }else{
                    ans += lt*rt;
                }
            }
            if(s[k] == '^')
            {
                if(isTrue)
                {
                    ans +=   rt*lf + rf*lt;
                }else{
                    ans += lt*rt + lf*rf;
                }
            }
            
        }
        return dp[i][j][isTrue] = ans;
        
    }
    int countWays(int N, string S){
        // code here
        memset(dp, -1, sizeof(dp));
        // step-1 initialize i, j
        return solve(S, 0, N-1,true);
    }
